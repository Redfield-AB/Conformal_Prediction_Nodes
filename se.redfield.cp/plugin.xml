<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

   <!-- This is an example for providing an own category 
      <extension
         point="org.knime.workbench.repository.categories">
      <category
            description="Category containing my nodes"
            icon="icons/default.png"
            level-id="myNodes"
            name="My nodes"
            path="/"/>
   </extension>
   -->
   
   <extension
         point="org.knime.workbench.repository.nodes">
      <node
            category-path="/community/conformalPrediction/advanced"
            factory-class="se.redfield.cp.nodes.ConformalPredictorTrainingLoopStartNodeFactory">
      </node>
      <node
            after="se.redfield.cp.nodes.ConformalPredictorTrainingLoopStartNodeFactory"
            category-path="/community/conformalPrediction/advanced"
            factory-class="se.redfield.cp.nodes.ConformalPredictorTrainingLoopEndNodeFactory">
      </node>
      <node
            after="se.redfield.cp.nodes.ConformalPredictorTrainingLoopEndNodeFactory"
            category-path="/community/conformalPrediction/advanced"
            factory-class="se.redfield.cp.nodes.ConformalPredictorLoopStartNodeFactory">
      </node>
      <node
            after="se.redfield.cp.nodes.ConformalPredictorLoopStartNodeFactory"
            category-path="/community/conformalPrediction/advanced"
            factory-class="se.redfield.cp.nodes.ConformalPredictorLoopEndNodeFactory">
      </node>
      <node
            after="se.redfield.cp.nodes.ConformalPredictorLoopEndNodeFactory"
            category-path="/community/conformalPrediction/advanced"
            factory-class="se.redfield.cp.nodes.ConformalPredictorCalibratorNodeFactory">
      </node>
      <node
            after="se.redfield.cp.nodes.ConformalPredictorCalibratorNodeFactory"
            category-path="/community/conformalPrediction/advanced"
            factory-class="se.redfield.cp.nodes.ConformalPredictorNodeFactory">
      </node>
      <node
            after="se.redfield.cp.nodes.ConformalPredictorNodeFactory"      
            category-path="/community/conformalPrediction/advanced"
            factory-class="se.redfield.cp.nodes.ConformalPredictorClassifierNodeFactory">
      </node>
      <node
            after="se.redfield.cp.nodes.ConformalPredictorClassifierNodeFactory"      
            category-path="/community/conformalPrediction/advanced"
            factory-class="se.redfield.cp.nodes.ConformalPredictorCalibratorRegressionNodeFactory">
      </node>
      <node
            after="se.redfield.cp.nodes.ConformalPredictorCalibratorRegressionNodeFactory"
            category-path="/community/conformalPrediction/advanced"
            factory-class="se.redfield.cp.nodes.ConformalPredictorRegressionNodeFactory">
      </node>
      <node
            category-path="/community/conformalPrediction"
            factory-class="se.redfield.cp.nodes.ConformalPartitionNodeFactory">
      </node>
      <node
            after="se.redfield.cp.nodes.ConformalPartitionNodeFactory" 
            category-path="/community/conformalPrediction"
            factory-class="se.redfield.cp.nodes.CompactConformalClassificationNodeFactory">
      </node>
      <node
            after="se.redfield.cp.nodes.CompactConformalClassificationNodeFactory" 
            category-path="/community/conformalPrediction"
            factory-class="se.redfield.cp.nodes.CompactConformalRegressionNodeFactory">
      </node>
      <node
            after="se.redfield.cp.nodes.CompactConformalRegressionNodeFactory" 
            category-path="/community/conformalPrediction"
            factory-class="se.redfield.cp.nodes.ConformalPredictorScorerNodeFactory">
      </node>
      <node
            after="se.redfield.cp.nodes.ConformalPredictorScorerNodeFactory" 
            category-path="/community/conformalPrediction"
            factory-class="se.redfield.cp.nodes.ConformalPredictorScorerRegressionNodeFactory">
      </node>
   </extension>
   <extension
         point="org.knime.workbench.repository.categories">
      <category
            icon="icons/redfield_logo.png"
            level-id="conformalPrediction"
            name="Conformal prediction"
            path="/community">
      </category>
            <category
            icon="icons/redfield_logo.png"
            level-id="advanced"
            name="Advanced"
            path="/community/conformalPrediction">
      </category>
   </extension>
   
</plugin>
